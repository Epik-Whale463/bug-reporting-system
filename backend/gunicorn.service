[Unit]
Description=Gunicorn instance to serve Django project
After=network.target

[Service]
User=www-data
Group=www-data
WorkingDirectory=/home/youruser/BugReportingSystem/backend
ExecStart=/home/youruser/.venv/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/youruser/BugReportingSystem/backend/gunicorn.sock backend.wsgi:application

[Install]
WantedBy=multi-user.target
